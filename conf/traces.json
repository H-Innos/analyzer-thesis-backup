{
  "ana": {
    "int": {
      "def_exc": true,
      "interval": false,
      "enums": true,
      "def_exc_widen_by_join": true,
      "refinement" : "once"
    },
    "malloc": {
      "wrappers": [
        "kmalloc",
        "__kmalloc",
        "usb_alloc_urb",
        "__builtin_alloca",
        "kzalloc",

        "ldv_malloc",

        "kzalloc_node",
        "ldv_zalloc",
        "kmalloc_array",
        "kcalloc",

        "ldv_xmalloc",
        "ldv_xzalloc",
        "ldv_calloc"
      ]
    },
    "base" : {
      "invariant": {
        "enabled": false
      },
      "eval": {
        "deep-query": false
      }
    }
  },
  "sem": {
    "unknown_function": {
      "invalidate": {
        "globals": false
      },
      "spawn": true
    },
    "builtin_unreachable": {
      "dead_code": true
    },
    "int": {
      "signed_overflow": "assume_none"
    }
  },
  "exp": {
    "priv-distr-init": false,
    "priv-prec-dump-proj" : true
  },
  "solvers" : {
    "td3": {
      "side_widen" : "sides-pp"
    }
  }
}
